@{
    ViewData["Title"] = "AuthorizedPage";
}
<h1>Authorized Page</h1>

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-bordered">
        @foreach(var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}

